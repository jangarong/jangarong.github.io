    "dev": "next dev",
    "build": "next build",
    "start": "serve static_dist",
    "start-electron": "electron-forge start",
    "lint": "next lint",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make-all": "npm run make-mac-x64 && npm run make-mac-arm && npm run make-win-x64 && npm run make-win-arm && npm run make-linux-x64 && npm run make-linux-arm",
    "make-mac-x64": "npm run make -- --arch=x64 --platform=darwin",
    "make-mac-arm": "npm run make -- --arch=arm64 --platform=darwin",
    "make-linux-x64": "npm run make -- --arch=x64 --platform=linux",
    "make-linux-arm": "npm run make -- --arch=arm64 --platform=linux",
    "make-win-x64": "npm run package -- --arch=x64 --platform=win32",
    "make-win-arm": "npm run package -- --arch=arm64 --platform=win32"
  },
